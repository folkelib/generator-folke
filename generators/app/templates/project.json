{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-rc1-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc1-final",
    "Microsoft.Extensions.DependencyInjection": "1.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final",
    "Folke.Identity.Elm": "1.0.0-beta-*",
    "Folke.Elm": "1.1.0-beta-*",
    "Folke.Elm.Sqlite": "1.1.0-beta-*",
    "Folke.CsTsService": "1.0.0-beta-*",
    "Folke.Mvc.Extensions": "1.0.0-beta-*",
    "Folke.Identity.Server": "1.0.0-beta-*"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "csts": "<%= name %>"
  },
  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp build" ]
  },
  "frameworks": {
    "dnx451": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
